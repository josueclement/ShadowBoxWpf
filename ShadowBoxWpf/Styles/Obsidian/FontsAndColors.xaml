<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="ShadowBox.Obsidian.Colors.Background1">#1E1E1E</Color>
    <Color x:Key="ShadowBox.Obsidian.Colors.Background2">#1F1F1F</Color>
    <Color x:Key="ShadowBox.Obsidian.Colors.Background3">#2E2E2E</Color>
    <Color x:Key="ShadowBox.Obsidian.Colors.Background4">#3D3D3D</Color>
    <Color x:Key="ShadowBox.Obsidian.Colors.Background5">#4F4F4F</Color>
    <Color x:Key="ShadowBox.Obsidian.Colors.Selection">#3399FF</Color>
    <Color x:Key="ShadowBox.Obsidian.Colors.Foreground">#DCDCDC</Color>
    <Color x:Key="ShadowBox.Obsidian.Colors.ForegroundDisabled">#AFAFAF</Color>
    <Color x:Key="ShadowBox.Obsidian.Colors.Error">#8C2F2F</Color>
    <Color x:Key="ShadowBox.Obsidian.Colors.Warning">#FFDBA3</Color>
    <Color x:Key="ShadowBox.Obsidian.Colors.Success">#57A64A</Color>

    <Color x:Key="Obsidian.Colors.Accent1.Normal">#115992</Color>
    <Color x:Key="Obsidian.Colors.Accent1.MouseOver">#1877b6</Color>
    <Color x:Key="Obsidian.Colors.Accent1.MouseDown">#0e3d5e</Color>
    <!--<Color x:Key="Obsidian.Colors.Accent1.MouseOver">#0969b5</Color>
    <Color x:Key="Obsidian.Colors.Accent1.MouseDown">#0470c5</Color>-->

    <Color x:Key="Obsidian.Colors.Accent2.Normal">#76b9ed</Color>
    <Color x:Key="Obsidian.Colors.Accent2.MouseOver">#6da9d8</Color>
    <Color x:Key="Obsidian.Colors.Accent2.MouseDown">#649ac3</Color>

    <Color x:Key="Obsidian.Colors.Error.Normal">#973232</Color>
    <Color x:Key="Obsidian.Colors.Error.MouseOver">#B03B3B</Color>
    <Color x:Key="Obsidian.Colors.Error.MouseDown">#652222</Color>

    <!-- New -->
    <Color x:Key="Obsidian.Colors.Background">#202020</Color>
    
    <!-- Button -->
    <Color x:Key="Obsidian.Colors.D1.Border">#353535</Color>
    <Color x:Key="Obsidian.Colors.D1.FillNormal">#2d2d2d</Color>
    <Color x:Key="Obsidian.Colors.D1.FillMouseOver">#323232</Color>
    <Color x:Key="Obsidian.Colors.D1.FillMouseDown">#272727</Color>
    <Color x:Key="Obsidian.Colors.D1.Text">#cecece</Color>
    <Color x:Key="Obsidian.Colors.D1.TextDisabled">#787878</Color>

    <!-- CheckBox -->
    <Color x:Key="Obsidian.Colors.D2.Border">#a8a8a8</Color>
    <Color x:Key="Obsidian.Colors.D2.FillNormal">#1d1d1d</Color>
    <Color x:Key="Obsidian.Colors.D2.FillMouseOver">#2a2a2a</Color>
    <Color x:Key="Obsidian.Colors.D2.FillMouseDown">#303030</Color>
    <Color x:Key="Obsidian.Colors.D2.Text">#eeeeee</Color>
    <Color x:Key="Obsidian.Colors.D2.TextDisabled">#717171</Color>

    <!-- ListBox -->
    <Color x:Key="Obsidian.Colors.D3.Border">#2b2b2b</Color>
    <Color x:Key="Obsidian.Colors.D3.FillNormal">#2b2b2b</Color>
    <Color x:Key="Obsidian.Colors.D3.FillMouseOver">#373737</Color>
    <Color x:Key="Obsidian.Colors.D3.FillMouseDown">#333333</Color>
    <Color x:Key="Obsidian.Colors.D3.Text">#e0e0e0</Color>
    <Color x:Key="Obsidian.Colors.D3.TextDisabled">#717171</Color>

    <SolidColorBrush x:Key="Obsidian.Brushes.Accent1.Normal" Color="{DynamicResource Obsidian.Colors.Accent1.Normal}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.Accent1.MouseOver" Color="{DynamicResource Obsidian.Colors.Accent1.MouseOver}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.Accent1.MouseDown" Color="{DynamicResource Obsidian.Colors.Accent1.MouseDown}" />

    <SolidColorBrush x:Key="Obsidian.Brushes.Accent2.Normal" Color="{DynamicResource Obsidian.Colors.Accent2.Normal}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.Accent2.MouseOver" Color="{DynamicResource Obsidian.Colors.Accent2.MouseOver}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.Accent2.MouseDown" Color="{DynamicResource Obsidian.Colors.Accent2.MouseDown}" />

    <SolidColorBrush x:Key="Obsidian.Brushes.Error.Normal" Color="{DynamicResource Obsidian.Colors.Error.Normal}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.Error.MouseOver" Color="{DynamicResource Obsidian.Colors.Error.MouseOver}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.Error.MouseDown" Color="{DynamicResource Obsidian.Colors.Error.MouseDown}" />

    <SolidColorBrush x:Key="Obsidian.Brushes.Background" Color="{DynamicResource Obsidian.Colors.Background}" />

    <SolidColorBrush x:Key="Obsidian.Brushes.D1.Border" Color="{DynamicResource Obsidian.Colors.D1.Border}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.D1.FillNormal" Color="{DynamicResource Obsidian.Colors.D1.FillNormal}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.D1.FillMouseOver" Color="{DynamicResource Obsidian.Colors.D1.FillMouseOver}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.D1.FillMouseDown" Color="{DynamicResource Obsidian.Colors.D1.FillMouseDown}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.D1.Text" Color="{DynamicResource Obsidian.Colors.D1.Text}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.D1.TextDisabled" Color="{DynamicResource Obsidian.Colors.D1.TextDisabled}" />

    <SolidColorBrush x:Key="Obsidian.Brushes.D2.Border" Color="{DynamicResource Obsidian.Colors.D2.Border}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.D2.FillNormal" Color="{DynamicResource Obsidian.Colors.D2.FillNormal}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.D2.FillMouseOver" Color="{DynamicResource Obsidian.Colors.D2.FillMouseOver}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.D2.FillMouseDown" Color="{DynamicResource Obsidian.Colors.D2.FillMouseDown}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.D2.Text" Color="{DynamicResource Obsidian.Colors.D2.Text}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.D2.TextDisabled" Color="{DynamicResource Obsidian.Colors.D2.TextDisabled}" />

    <SolidColorBrush x:Key="Obsidian.Brushes.D3.Border" Color="{DynamicResource Obsidian.Colors.D3.Border}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.D3.FillNormal" Color="{DynamicResource Obsidian.Colors.D3.FillNormal}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.D3.FillMouseOver" Color="{DynamicResource Obsidian.Colors.D3.FillMouseOver}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.D3.FillMouseDown" Color="{DynamicResource Obsidian.Colors.D3.FillMouseDown}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.D3.Text" Color="{DynamicResource Obsidian.Colors.D3.Text}" />
    <SolidColorBrush x:Key="Obsidian.Brushes.D3.TextDisabled" Color="{DynamicResource Obsidian.Colors.D3.TextDisabled}" />

    <SolidColorBrush x:Key="ShadowBox.Obsidian.Brushes.Background1" Color="{DynamicResource ShadowBox.Obsidian.Colors.Background1}" />
    <SolidColorBrush x:Key="ShadowBox.Obsidian.Brushes.Background2" Color="{StaticResource ShadowBox.Obsidian.Colors.Background2}" />
    <SolidColorBrush x:Key="ShadowBox.Obsidian.Brushes.Background3" Color="{StaticResource ShadowBox.Obsidian.Colors.Background3}" />
    <SolidColorBrush x:Key="ShadowBox.Obsidian.Brushes.Background4" Color="{StaticResource ShadowBox.Obsidian.Colors.Background4}" />
    <SolidColorBrush x:Key="ShadowBox.Obsidian.Brushes.Background5" Color="{StaticResource ShadowBox.Obsidian.Colors.Background5}" />
    <SolidColorBrush x:Key="ShadowBox.Obsidian.Brushes.Selection" Color="{StaticResource ShadowBox.Obsidian.Colors.Selection}" />
    <SolidColorBrush x:Key="ShadowBox.Obsidian.Brushes.Foreground" Color="{StaticResource ShadowBox.Obsidian.Colors.Foreground}" />
    <SolidColorBrush x:Key="ShadowBox.Obsidian.Brushes.ForegroundDisabled" Color="{StaticResource ShadowBox.Obsidian.Colors.ForegroundDisabled}" />
    <SolidColorBrush x:Key="ShadowBox.Obsidian.Brushes.Error" Color="{StaticResource ShadowBox.Obsidian.Colors.Error}" />
    <SolidColorBrush x:Key="ShadowBox.Obsidian.Brushes.Warning" Color="{StaticResource ShadowBox.Obsidian.Colors.Warning}" />
    <SolidColorBrush x:Key="ShadowBox.Obsidian.Brushes.Success" Color="{StaticResource ShadowBox.Obsidian.Colors.Success}" />

    <FontFamily x:Key="ShadowBox.Obsidian.Fonts.MainFont">Segoe UI, Lucida Sans Unicode, Verdana</FontFamily>

